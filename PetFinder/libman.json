{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jqueryui@1.12.1",
      "destination": "wwwroot/lib/"
    },
    {
      "provider": "jsdelivr",
      "library": "fullcalendar@5.1.0",
      "destination": "wwwroot/fullcalendar/"
    },
    {
      "library": "fontawesome-iconpicker@3.2.0",
      "destination": "wwwroot/fontawesome-iconpicker/"
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.css",
        "css/bootstrap-datepicker.min.css",
        "css/bootstrap-datepicker.standalone.min.css"
      ]
    },
    {
      "library": "slick-carousel@1.8.1",
      "destination": "wwwroot/slick-carousel/"
    },
    {
      "library": "fotorama@4.6.4",
      "destination": "wwwroot/fotorama/"
    },
    {
      "library": "chosen@1.8.7",
      "destination": "wwwroot/chosen/"
    },
    {
      "library": "feather-icons@4.28.0",
      "destination": "wwwroot/feather-icons/"
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/datatables/",
      "files": [
        "js/jquery.dataTables.min.js",
        "css/dataTables.jqueryui.css",
        "css/dataTables.jqueryui.min.css",
        "css/jquery.dataTables.css",
        "css/jquery.dataTables.min.css"
      ]
    }
  ]
}